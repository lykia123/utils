{
  "name": "@ccprivate/utils",
  "version": "0.0.8",
  "main": "dist/ccUtil.cjs.js",
  "module": "dist/ccUtil.es.js",
  "browser": "dist/ccUtil.umd.js",
  "description": "工具类库",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "lint": "eslint src test --fix",
    "test": "jest --coverage",
    "test:ui": "npx majestic",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "majestic": {
    "env": {
      "NODE_ENV": "test"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "npm run test -- --bail --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-power-assert": "^3.0.0",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.0",
    "power-assert": "^1.6.1",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.0.0"
  },
  "engines": {
    "node": ">=13.12.0",
    "npm": ">=5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.skysri.com/FrontEnd/utils.git"
  },
  "bugs": {
    "url": "http://gitlab.skysri.com/FrontEnd/utils/issues"
  }
}
